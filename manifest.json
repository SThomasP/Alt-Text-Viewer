{
  "manifest_version": 2,
  "name": "Alt Text Viewer",
  "version": "1.0",

  "icons": {
   "48": "icons/icon-48.png",
   "96": "icons/icon-96.png"
 },

  "description": "Shows the alt text seperately beneath the image.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["show-alt-text.js"]
    }
  ]

}